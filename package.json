{
  "name": "joplin-plugin-containers",
  "version": "1.0.0",
  "scripts": {
    "dist": "webpack --env joplin-plugin-config=buildMain && webpack --env joplin-plugin-config=buildExtraScripts && webpack --env joplin-plugin-config=createArchive",
    "prepare": "pnpm run dist",
    "update": "pnpm install -g generator-joplin && yo joplin --node-package-manager pnpm --update --force",
    "updateVersion": "webpack --env joplin-plugin-config=updateVersion"
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin",
    "container",
    "containers"
  ],
  "devDependencies": {
    "@joplin/lib": "~2.9",
    "@types/node": "^18.7.13",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "on-build-webpack": "^0.1.0",
    "tar": "^6.1.11",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yargs": "^16.2.0"
  },
  "files": [
    "publish"
  ]
}
